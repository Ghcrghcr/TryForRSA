{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc.exe 生成活动文件",
			"command": "gcc",
			"args": [
				"-g",
				"${file}",
				"-o",
				"${fileDirname}\\${fileBasenameNoExtension}.exe",
				"-I",
				"E:\\code\\gmp\\include",
				"-L",
				"E:\\code\\gmp\\lib",
				"-lgmpxx",
				"-lgmp"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "编译器: C:\\TDM-GCC-64\\bin\\gcc.exe"
		}
	]
}